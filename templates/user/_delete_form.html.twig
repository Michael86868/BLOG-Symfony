<form class="d-inline" method="post" action="{{ path('user_delete', {'id': user.id}) }}" onsubmit="return confirm('Jsi si opravdu jist, že chceš odstranit tento účet ? Tato akce je nevratná !');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="p-0 btn text-danger" style=""><span class="fa fa-trash icon"></span></button>
</form>
